{
  "name": "hipchat-dance-party-service",
  "version": "0.0.5",
  "private": true,
  "engines": {
    "node": "v6.4.0"
  },
  "scripts": {
    "deploy": "npm run docker:build && npm run docker:push && npm run docker:deploy",
    "docker:build": "docker build -t gcr.io/my-kubernetes-1269/hipchat-dance-party-service:v$(node -e \"console.log(require('./package.json').version)\") .",
    "docker:push": "gcloud docker -- push gcr.io/my-kubernetes-1269/hipchat-dance-party-service:v$(node -e \"console.log(require('./package.json').version)\")",
    "docker:deploy": "kubectl rolling-update hipchat-dance-ctrl --image-pull-policy Always --image=gcr.io/my-kubernetes-1269/hipchat-dance-party-service:v$(node -e \"console.log(require('./package.json').version)\")",
    "build": "./bin/build.sh",
    "heroku-postbuild": "./bin/build.sh",
    "dev": "nodemon -w app.js -w atlassian-connect.json -w config.json -w routes/index.js app.js",
    "start": "node app.js",
    "test": "TZ=GMT NODE_ENV=test mocha"
  },
  "dependencies": {
    "atlassian-connect-express": "^2.0.0",
    "atlassian-connect-express-hipchat": "^0.3.8",
    "atlassian-connect-express-redis": "^0.1.8",
