    "express": "^4.14.0",
    "express-hbs": "^1.0.3",
    "express-promise-router": "^1.1.1",
    "hbs-json": "^0.1.1",
    "isomorphic-fetch": "^2.2.1",
    "jugglingdb-mysql": "^0.2.3",
    "lodash": "^4.17.4",
    "morgan": "^1.7.0",
    "request": "^2.79.0",
    "rollbar": "^0.6.3",
    "rsvp": "^3.3.3",
    "spotify-web-api-node": "^2.3.6",
    "static-expiry": "^0.0.11",
    "string.prototype.repeat": "^0.2.0",
    "uuid": "^3.0.1"
  },
