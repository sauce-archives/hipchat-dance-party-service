{
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "plugins": [ "react" ],
  "parser": "babel-eslint",
  "extends": [ "semistandard", "eslint:recommended", "plugin:react/recommended" ],
  "rules": {
    "no-unused-vars": [2, { "vars": "all", "args": "after-used" }],
    "no-var": 2,
    "no-invalid-this": 2,
    "eqeqeq": 2,
    "handle-callback-err": 2,
    "consistent-return": 2,
    "comma-dangle": 2
  },
  "env": {
    "browser": true,
    "es6": true
  },
  "globals": {
    "fetch": false
  }
}
